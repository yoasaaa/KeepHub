{
  "name": "keephub",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup-project": "npm i && cd client && npm i",
    "setup-production-app": "npm i && cd client && npm i && npm run build && mkdir -p public && cp -r client/dist/* ./public/",
    "build": "npm install && cd client && npm install && npm run build && cd .. && mkdir -p public && cp -r client/dist/* ./public/",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "populate": "node populate.js"
  },
  "keywords": ["resource management", "digital resources", "mern stack"],
  "author": "Yichen Yang",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.0",
    "cloudinary": "^1.37.3",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.6",
    "datauri": "^4.1.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.8.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "nodemon": "^3.1.10",
    "url-parse": "^1.5.10"
  }
}
